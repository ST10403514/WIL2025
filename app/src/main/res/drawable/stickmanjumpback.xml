<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">
    
    <!-- Stick figure jumping backward -->
    <!-- Head -->
    <circle
        android:fillColor="#FFFFFF"
        android:strokeColor="#000000"
        android:strokeWidth="2"
        android:cx="50"
        android:cy="20"
        android:r="8" />
    
    <!-- Body -->
    <path
        android:strokeColor="#000000"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M50,28 L50,60" />
    
    <!-- Arms (reaching backward) -->
    <path
        android:strokeColor="#000000"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M50,35 L35,25" />
    <path
        android:strokeColor="#000000"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M50,35 L35,30" />
    
    <!-- Legs (jumping backward motion) -->
    <path
        android:strokeColor="#000000"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M50,60 L35,80" />
    <path
        android:strokeColor="#000000"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M50,60 L40,85" />
    
    <!-- Motion lines to show backward movement -->
    <path
        android:strokeColor="#666666"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:pathData="M70,25 L85,25" />
    <path
        android:strokeColor="#666666"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:pathData="M70,30 L85,30" />
    <path
        android:strokeColor="#666666"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:pathData="M70,35 L85,35" />
    
</vector>
